{
  "name": "@innkeeper/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsx watch workers/index.ts\" --names \"WEB,WORKER\" --prefix-colors \"blue,magenta\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:watch:file": "powershell -ExecutionPolicy Bypass -File scripts/test-watch-output.ps1 unit",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:watch:all": "pnpm test:watch",
    "test:output": "powershell -ExecutionPolicy Bypass -File scripts/test-output.ps1 all",
    "test:e2e:output": "powershell -ExecutionPolicy Bypass -File scripts/test-e2e-output.ps1",
    "start-worker": "tsx workers/index.ts",
    "network:local": "node scripts/set-network.js local",
    "network:testnet": "node scripts/set-network.js testnet",
    "network:mainnet": "node scripts/set-network.js mainnet"
  },
  "dependencies": {
    "@base-org/account": "^1.0.0",
    "@coinbase/wallet-sdk": "^3.7.2",
    "@farcaster/auth-kit": "^0.8.1",
    "@farcaster/miniapp-sdk": "^0.1.0",
    "@farcaster/miniapp-wagmi-connector": "^1.1.0",
    "@innkeeper/agents": "workspace:*",
    "@innkeeper/engine": "workspace:*",
    "@innkeeper/lib": "workspace:*",
    "@privy-io/react-auth": "^1.0.0",
    "@safe-global/safe-apps-provider": "^0.18.0",
    "@safe-global/safe-apps-sdk": "^9.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.11",
    "@tokenbound/sdk": "^0.1.2",
    "@vercel/og": "^0.6.1",
    "bullmq": "^5.3.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.555.0",
    "next": "16.0.5",
    "next-mdx-remote": "^5.0.0",
    "openai": "^6.9.1",
    "pinata": "^2.5.1",
    "pino": "^10.0.0",
    "pixi.js": "^7.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "thread-stream": "^3.1.0",
    "viem": "^2.40.3",
    "wagmi": "^2.19.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "desm": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "16.0.5",
    "fastbench": "^1.0.1",
    "nodemon": "^3.0.2",
    "pino-elasticsearch": "^8.1.0",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tap": "^21.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}